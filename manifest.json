{
  "name": "Cloudflare Debugger",
  "version": "0.0.1",
  "description": "A little debug tool for Cloudflare",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "lib/jquery-3.1.1.min.js",
      "js/background.js",
      "js/global_static.js",
      "js/webRequestListener.js",
      "js/webRequestListenerHelper.js",
      "js/class/WebRequest.js",
      "js/class/NetworkRequest.js"
    ]
  },
  "commands": { "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+5",
        "mac": "Command+Shift+0",
        "windows": "Ctrl+Shift+0"
      },
      "description": "Toggle feature foo"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "img/cloudflare-logo.png",
    "default_popup": "popup.html",
    "default_title": "Cloudflare Debugger"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "notifications",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "debugger",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "css/tooltipster.bundle.min.css",
    "lib/tooltipster.bundle.min.js",
		"css/balloon.min.css"
  ]
}
