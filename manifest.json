{
  "name": "Doctorflare",
  "version": "0.0.1",
  "description": "A little debug tool for Cloudflare",
  "manifest_version": 2,
  "background": {
    "persistent": true,
    "scripts": [
      "lib/jquery-3.1.1.min.js",
      "js/background.js",
      "js/global_static.js",
      "js/webRequestListener.js",
      "js/class/NetworkRequest.js"
    ]
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "img/cf-logo-v-rgb-rev.png",
    "default_popup": "popup.html",
    "default_title": "Doctorflare"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "notifications",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "debugger",
    "<all_urls>"
  ]
}
